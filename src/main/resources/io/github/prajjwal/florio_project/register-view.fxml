<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="406.0" prefWidth="666.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.prajjwal.florio_project.controller.RegisterController">
   <children>
      <Label text="Register">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" prefHeight="31.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Email">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="8.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Name">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="8.0" />
                     </VBox.margin>
                  </Label>
                  <Label layoutX="41.0" layoutY="44.0" text="Phone">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="8.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Password">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox prefHeight="75.0" prefWidth="165.0">
               <children>
                  <TextField fx:id="emailTextField">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="nameTextField">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="nameTextField1" layoutX="10.0" layoutY="40.0" text="+91">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </TextField>
                  <PasswordField fx:id="registerPasswordField" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="registerButton" mnemonicParsing="false" text="Register" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" top="20.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="registerLoginButton" mnemonicParsing="false" onAction="#onRegisterLoginButtonClick" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
