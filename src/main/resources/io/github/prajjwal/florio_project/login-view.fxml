<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="io.github.prajjwal.florio_project.controller.LoginController">
    <children>
        <ImageView fitHeight="96.0" fitWidth="189.0" pickOnBounds="true" preserveRatio="true">
            <image>
            </image>
        </ImageView>
        <Label text="Login Page">
            <font>
                <Font size="17.0" />
            </font>
            <VBox.margin>
                <Insets bottom="20.0" top="20.0" />
            </VBox.margin>
        </Label>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="600.0">
            <children>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Label text="Phone or Email">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Label>
                        <Label contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" text="Password">
                            <font>
                                <Font size="14.0" />
                            </font>
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </Label>
                    </children>
                    <HBox.margin>
                        <Insets right="5.0" />
                    </HBox.margin>
                </VBox>
                <VBox maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="64.0" prefWidth="196.0">
                    <children>
                        <TextField />
                        <PasswordField>
                            <VBox.margin>
                                <Insets top="12.0" />
                            </VBox.margin>
                        </PasswordField>
                    </children>
                </VBox>
            </children>
        </HBox>
        <Button mnemonicParsing="false" prefHeight="32.0" prefWidth="72.0" text="Login">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" prefHeight="32.0" prefWidth="72.0" text="Register">
            <VBox.margin>
                <Insets top="15.0" />
            </VBox.margin>
        </Button>
    </children>
</VBox>
